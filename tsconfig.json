{
  "compilerOptions": {
    "outDir": "dist",
    "module": "esnext",
    "target": "es5",
    "lib": [
      "es6",
      "es2016",
      "es2017",
      "dom"
    ],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "suppressImplicitAnyIndexErrors": true,
    "baseUrl": "src"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
  ]
}
