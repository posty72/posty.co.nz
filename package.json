{
  "name": "posty",
  "version": "1.0.0",
  "private": true,
  "description": "posty",
  "author": "Josh Post",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run format && npm run lint:spelling",
    "lint:eslint": "eslint \"./**/*.{js,ts,tsx}\" --fix --quiet && prettier --write --loglevel error \"./**/*.{js,jsx,json,css,scss,html,ts,tsx}\"",
    "lint:stylelint": "stylelint --fix \"./src/**/*.{css,scss}\"",
    "lint:spelling": "cspell \"*/**/*\"",
    "format": "prettier --write --loglevel error \"./**/*.{js,jsx,json,css,scss,html,ts,tsx}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "cssnano": "^5.0.8",
    "gatsby": "^3.13.0",
    "gatsby-plugin-image": "^1.13.0",
    "gatsby-plugin-manifest": "^3.13.0",
    "gatsby-plugin-postcss": "^4.13.0",
    "gatsby-plugin-react-helmet": "^4.13.0",
    "gatsby-plugin-robots-txt": "^1.6.10",
    "gatsby-plugin-sass": "^4.13.0",
    "gatsby-plugin-sharp": "^3.15.0",
    "gatsby-plugin-sitemap": "^4.9.0",
    "gatsby-source-filesystem": "^3.13.0",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^3.15.0",
    "node-sass": "^4.14.1",
    "postcss": "^8.3.6",
    "postcss-color-function": "^4.1.0",
    "postcss-css-variables": "^0.18.0",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-remify": "^1.0.1",
    "postcss-scss": "^4.0.0",
    "postcss-simple-vars": "^6.0.3",
    "postcss-strip-inline-comments": "^0.1.5",
    "precss": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-headroom": "^3.1.1",
    "react-helmet": "^6.1.0",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.2",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "cspell": "^5.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "husky": "^7.0.2",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0"
  }
}
